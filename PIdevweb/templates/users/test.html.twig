{% extends 'base.html.twig' %}

{% block title %}Edit Users{% endblock %}

{% block body %}
    <form method="post">
        <div class="form-group">
            <label>email</label>
            <input class="form-control p_input" type="email" name="email" id="email">
        </div>
        <div class="form-group">
            <label>Password</label>
            <input class="form-control p_input" type="password" name="password" id="pass">
        </div>

        <div class="text-center">
            <button type="submit" class="btn btn-primary btn-block enter-btn">Submit</button>
        </div>
    </form>
{% endblock %}